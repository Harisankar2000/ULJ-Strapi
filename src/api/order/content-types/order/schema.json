{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "required": true,
      "type": "integer"
    },
    "quantity": {
      "type": "integer"
    },
    "orderStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "processing",
        "finishing"
      ],
      "required": true,
      "default": "pending"
    },
    "userState": {
      "type": "enumeration",
      "enum": [
        "Odisha",
        "Maharashtra",
        "Telengana"
      ],
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    }
  }
}
